{
  "name": "greybots-signin",
  "version": "1.0.0",
  "description": "Clock in/out system for Team 973 The Greybots.",
  "main": "main.js",
  "scripts": {
    "autolint": "fixmyjs public/",
    "lint": "jshint public/ --exclude-path .gitignore",
    "test": "jshint public/ --exclude-path .gitignore",
    "serve": "browserify ./public/js/main/renderer.js > ./public/js/main/bundle.js && firebase serve",
    "deploy": "browserify ./public/js/main/renderer.js > ./public/js/main/bundle.js && firebase deploy",
    "start": "browserify ./public/js/main/renderer.js > ./public/js/main/bundle.js && electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team973/greybots-signin"
  },
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "firebase": "^5.3.0"
  },
  "author": "Chris Lawson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Team973/greybots-signin/issues"
  },
  "homepage": "https://github.com/Team973/greybots-signin#readme",
  "jshintConfig": {
    "esversion": 6,
    "strict": true
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "electron": "^2.0.0",
    "firebase-tools": "^4.0.0",
    "fixmyjs": "^2.0.0",
    "jshint": "^2.9.5"
  }
}
