{
  "name": "greybots-signin",
  "version": "1.0.0",
  "description": "Clock in/out system for Team 973 The Greybots.",
  "main": "main.js",
  "scripts": {
    "lint": "eslint .",
    "serve": "browserify ./public/js/renderer.js > ./public/js/bundle.js && firebase serve",
    "deploy": "browserify ./public/js/renderer.js > ./public/js/bundle.js && firebase deploy",
    "start": "browserify ./public/js/renderer.js > ./public/js/bundle.js && electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team973/greybots-signin"
  },
  "dependencies": {
    "firebase": "^5.3.0"
  },
  "author": "Chris Lawson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Team973/greybots-signin/issues"
  },
  "homepage": "https://github.com/Team973/greybots-signin#readme",
  "jshintConfig": {
    "esversion": 6,
    "strict": true
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "electron": "^2.0.0",
    "electron-rebuild": "^1.8.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "firebase-tools": "^4.0.0"
  }
}
